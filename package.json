{
  "name": "mongoose-transform-field-plugin",
  "version": "1.0.0",
  "description": "Easily apply field transformation middleware",
  "main": "dist/plugin.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "npm run typecheck -- --watch",
    "lint": "tslint -p ./tsconfig.test.json -s node_modules/custom-tslint-formatters/formatters -t grouped",
    "lint:fix": "npm run lint -- --fix",
    "test": "rimraf coverage && nyc mocha --check-leaks --exit --full-trace -r ts-node/register -r source-map-support/register test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alorel/mongoose-transform-field-plugin.git"
  },
  "keywords": [
    "mongoose",
    "plugin",
    "field",
    "transform",
    "middleware"
  ],
  "author": {
    "name": "Arturas Molcanovas",
    "url": "https://github.com/Alorel",
    "email": "a.molcanovas@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alorel/mongoose-transform-field-plugin/issues"
  },
  "homepage": "https://github.com/Alorel/mongoose-transform-field-plugin",
  "dependencies": {
    "@types/mongoose": "^5.0.0"
  },
  "peerDependencies": {
    "mongoose": "^5.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/dotenv": "^4.0.2",
    "@types/lodash": "^4.14.102",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.2",
    "@types/uuid": "^3.4.3",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "custom-tslint-formatters": "^2.1.1",
    "dotenv": "^5.0.0",
    "lodash": "^4.17.5",
    "mocha": "^5.0.0",
    "mongoose": "^5.0.3",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.3",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1",
    "uuid": "^3.2.1"
  }
}
